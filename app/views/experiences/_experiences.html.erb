<% experiences.each do |experience| %>
  <h3>Job title: <%= experience.job_title %></h3>
  <h3>Company: <%= experience.company %></h3>
  <h3>Start at: <%= experience.start_date %></h3>
  <h3>End at: <%= experience.end_date %></h3>
  <h3>Current : <%=  experience.current ? "Yes" : "No" %></h3>
  <h3>Description: <%= experience.description %></h3>

   <a href="#">Edit</a>
   <a href="#">Delete</a>
   <hr>
<% end %>
