<h1>title: <%= @project.title %></h1>
<h1>description: <%= @project.description %></h1>
<h1>web_app_link: <%= @project.web_app_link %></h1>
<h1>github_link: <%= @project.github_link %></h1>
<%= image_tag @project.logo.url(:medium) %>

<p>Tags: <%= @project.project_tags.map {|t| t.title.capitalize }.join(", ") %></p>


<%= link_to "Edit", edit_user_profile_project_path(current_user, @profile, @project) %>
<%= link_to "Delete", user_profile_project_path(current_user, @profile, @project),
                      method: :delete,
                      data:   {confirm: "Are you sure?"} %>
